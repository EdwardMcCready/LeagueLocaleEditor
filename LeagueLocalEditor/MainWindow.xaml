<Window x:Class="LeagueLocalEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeagueLocalEditor" 
        xmlns:viewmodels="clr-namespace:LeagueLocalEditor.ViewModels"
        mc:Ignorable="d"
        Title="Locale Editor" Height="450" Width="800">
    <Window.DataContext>
        <viewmodels:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <Button Command="{Binding RunLeagueCommand}"/>
    </Grid>
</Window>
